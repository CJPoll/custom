priority -50

snippet gs "GenServer"
	defmodule ${1:module} do
		use GenServer

		defmodule State do
			defstruct []
		end

		def start() do
			GenServer.start(__MODULE__, [])
		end

		def start_link() do
			GenServer.start_link(__MODULE__, [])
		end

		def stop(ref) do
			GenServer.stop(ref)
		end

		def init(_args) do
			{:ok, %State{}}
		end
	end
endsnippet

snippet def "def"
	def ${1:name}(${2:args}) do
		${3:contents}
	end
